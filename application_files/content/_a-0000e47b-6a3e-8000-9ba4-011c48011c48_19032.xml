    <versionUuid>_a-0000e47b-6a3e-8000-9ba4-011c48011c48_19043</versionUuid>
    <rule>
        <name>MW_AM_FormatUsername</name>
        <uuid>_a-0000e47b-6a3e-8000-9ba4-011c48011c48_19032</uuid>
        <description>Given a user, returns a properly formatted first and last name</description>
        <parentUuid>_a-0000e47b-6a3e-8000-9ba4-011c48011c48_16256</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>user(ri!user, "firstName") &amp; " " &amp; user(ri!user, "lastName")
</definition>
        <namedTypedValue>
            <name>user</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
