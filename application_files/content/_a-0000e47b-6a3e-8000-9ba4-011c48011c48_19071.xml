<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e47b-6a3e-8000-9ba4-011c48011c48_19082</versionUuid>
    <rule>
        <name>MW_AM_TestLengthValidation</name>
        <uuid>_a-0000e47b-6a3e-8000-9ba4-011c48011c48_19071</uuid>
        <description>Given a text string, prompts error if max character length is exceeded </description>
        <parentUuid>_a-0000e47b-6a3e-8000-9ba4-011c48011c48_16256</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  len(ri!string) &gt; ri!maxLength,
  "Cannot exceed " &amp; ri!maxLength &amp; " characters!",
  null
)</definition>
        <namedTypedValue>
            <name>string</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>maxLength</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
